msgid "{file_name} identified"
msgstr "{file_name} identificato"

msgid "{files_counted} file from {in_folder} folder identified"
msgid_plural "{files_counted} files from {in_folder} folder identified"
msgstr[0] "{files_counted} fila dalla cartella {in_folder} identificata"
msgstr[1] "{files_counted} file dalla cartella {in_folder} identificati"

msgid "Checksum"
msgstr "Somma di controllo"

msgid "Entire file content read"
msgstr "Lettura dell'intero contenuto del file"

msgid "Error encountered when trying to interpret JSON"
msgstr "Si è verificato un errore durante il tentativo di interpretare JSON"

msgid "File \"{file_name}\" has the following characteristics: {file_statistics}"
msgstr "Il file \"{file_name}\" ha le seguenti caratteristiche: {file_statistics}"

msgid "File \"{file_name}\" which has the {created_or_modified} datetime as \"{file_datetime}\" vs. \"{reference_datetime}\" has a verdict = {verdict}"
msgstr "Il file \"{file_name}\" che ha il {create_or_modified} datetime come \"{file_datetime}\" contro \"{reference_datetime}\" ha un verdetto = {verdict}"

msgid "File matching pattern identified"
msgstr "Identificazione del modello corrispondente dei file"

msgid "File {file_name} does not exist"
msgstr "Il file {file_name} non esiste"

msgid "File {file_name} has just been opened"
msgstr "Il file {file_name} è stato appena aperto"

msgid "File {file_name} has just been overwritten as {new_file_name}"
msgstr "Il file {file_name} è stato appena sovrascritto come {new_file_name}"

msgid "File {file_name} has just been renamed as {new_file_name}"
msgstr "Il file {file_name} è stato appena rinominato come {new_file_name}"

msgid "File {file_name} will be overwritten"
msgstr "Il file {nome_file} verrà sovrascritto"

msgid "File {file_name} will be renamed as {new_file_name}"
msgstr "Il file {file_name} verrà rinominato come {new_file_name}"

msgid "Folder {folder_name} does not exist"
msgstr "La cartella {folder_name} non esiste"

msgid "JSON structure interpreted"
msgstr "Struttura JSON interpretata"

msgid "Listing all files within {in_folder} folder looking for {matching_pattern} as matching pattern"
msgstr "Elencare tutti i file all'interno della cartella {in_folder} alla ricerca di {matching_pattern} come modello corrispondente"

msgid "Specific file name provided"
msgstr "Nome specifico del file fornito"

msgid "Unknown file datetime choice, expected is one of the following choices \"{implemented_choices}\" but provided was \"{created_or_modified}\"..."
msgstr "Scelta datetime del file sconosciuta, prevista è una delle seguenti scelte \"{implemented_choices}\" ma fornita estata \"{created_or_modified}\"..."

msgid "Unknown file type provided, expected either \"json\" or \"raw\", but got {in_file_type}"
msgstr "Tipo di file sconosciuto fornito, previsto \"json\" o \"raw\", ma ottenuto {in_file_type}"

msgid "created"
msgstr "creata"

msgid "date when created"
msgstr "data di creazione"

msgid "date when last modified"
msgstr "data dell'ultima modifica"

msgid "newer"
msgstr "più nuova"

msgid "older"
msgstr "più vecchia"

msgid "last modified"
msgstr "ultima modifica"

msgid "same"
msgstr "stessa"

msgid "size [bytes]"
msgstr "dimensione [bytes]"

msgid "unknown"
msgstr "sconosciuta"
